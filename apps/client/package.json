{
  "name": "@exchange/client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "prebuild": "rimraf dist",
    "preview": "vite preview",
    "build:prod": "build",
    "build:csr": "tsc & vite build",
    "build:ssr": "tsc & vite build --ssr ./src/entry-server.tsx --config vite.config.ssr.ts",
    "build": "npm run prebuild & npm run build:csr & npm run build:ssr",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "api-gen": "openapi --input http://localhost:5000/api/docs-json --name ApiService --client axios --useOptions useUnionTypes --output ./src/core/api",
    "clean": "rimraf ./build",
    "check-types": "tsc",
    "eslint": "eslint --cache --ext .ts . --config .eslintrc.js",
    "lint": "npm run eslint && npm run check-types",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx  . --fix --config .eslintrc.js",
    "check-types:client": "tsc --config ./tsconfig.json",
    "prettier:fix": "prettier --write .",
    "test": "jest"
  },
  "dependencies": {
    "@astral/ui": "^2.14.0",
    "@emotion/cache": "11.7.1",
    "@emotion/react": "11.9.0",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.8.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/base": "^5.0.0-alpha.104",
    "@mui/lab": "^5.0.0-alpha.106",
    "@mui/material": "^5.10.12",
    "@tanstack/react-query": "^4.26.1",
    "axios": "^1.3.4",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "ky": "^0.33.2",
    "openapi-typescript-codegen": "^0.23.0",
    "react-dom": "^18.2.0",
    "react-favicon": "^1.0.1",
    "react-hook-form": "^7.43.5",
    "react-lazy-load-image-component": "^1.5.5",
    "react-router-dom": "^6.3.0",
    "react-top-loading-bar": "^2.3.1",
    "vite-plugin-commonjs": "^0.6.2",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@types/node": "18.11.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb-typescript": "~17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "~6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "~7.31.11",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4",
    "vite": "^4.1.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "volta": {
    "node": "18.13.0",
    "npm": "8.19.2",
    "yarn": "1.19.2"
  }
}
