{
  "name": "exchange",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "npm install & npm run prisma:gen",
    "dev": "nest start --watch",
    "prebuild": "rimraf dist",
    "server:build": "nest build",
    "build:csr": "vite build",
    "build:ssr": "vite build --ssr src/entry-server.tsx --config vite.config.ssr.ts",
    "build:client": "npm run build:csr & npm run build:ssr",
    "build": "npm run server:build & npm run build:client",
    "build:start": "npm run prebuild & npm run build & npm run start:prod",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:origin": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "clean": "rimraf ./build",
    "check-types": "tsc",
    "eslint": "eslint --cache --ext .ts . --config .eslintrc.js",
    "lint": "npm run eslint && npm run check-types",
    "lint:fix": "eslint --cache --ext .ts . --fix --config .eslintrc.js",
    "check-types:client": "tsc --config ./client/tsconfig.json",
    "eslint:client": "eslint --cache --ext .js,.jsx,.ts,.tsx client/ --config ./client/.eslintrc.js --ignore-path ./client/.eslintignore",
    "lint:client": "npm run eslint:client && npm run check-types:client",
    "lint:fix-client": "eslint --cache --ext .js,.jsx,.ts,.tsx client/ --config ./client/.eslintrc.js --fix --ignore-path ./client/.eslintignore",
    "prettier:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:init": "npx prisma init --datasource-provider mongodb",
    "prisma:gen": "npx prisma generate",
    "prisma:drop": "npx prisma prisma db drop",
    "prisma:studio": " prisma studio",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "seed": "ts-node ./prisma/seeds/seed.ts",
    "plop": "npx plop",
    "infra": "cd ./devops && docker-compose up",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/material": "^5.11.12",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/throttler": "^4.0.0",
    "@prisma/client": "^4.11.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cron": "^2.2.0",
    "dotenv": "^16.0.3",
    "helmet": "^6.0.1",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.0.2",
    "react-dom": "^18.2.0",
    "react-favicon": "^1.0.1",
    "react-hook-form": "^7.43.5",
    "react-lazy-load-image-component": "^1.5.5",
    "react-router-dom": "^6.3.0",
    "react-top-loading-bar": "^2.3.1",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "telegraf": "^4.11.2",
    "tslog": "^4.7.5",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/mongoose": "^5.11.97",
    "@types/node": "18.11.18",
    "@types/node-cron": "^3.0.7",
    "@types/react-dom": "^18.0.6",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "commitlint": "^17.4.4",
    "eslint": "8.26.0",
    "eslint-config-airbnb-typescript": "~17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "~6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "~7.31.11",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.0",
    "jest": "29.3.1",
    "lint-staged": "^13.1.2",
    "plop": "^3.1.2",
    "prettier": "^2.3.2",
    "prisma": "^4.11.0",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-json-schema-generator": "^3.1.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4",
    "vite": "^4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "volta": {
    "node": "18.13.0",
    "npm": "8.19.2",
    "yarn": "1.19.2"
  }
}
